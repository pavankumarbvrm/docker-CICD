{
  "taskDefinition": {
    "family": "docker-nginx",
    "containerDefinitions": [
      {
        "name": "docker-nginx",
        "image": "984242085940.dkr.ecr.us-east-1.amazonaws.com/docker-nginx:latest",
        "cpu": 100,
        "portMappings": [
          {
            "protocol": "tcp",
            "containerPort": 80,
            "hostPort": 80
          }
        ],
        "memory": 128,
        "essential": true
      }
    ]
  }
}
